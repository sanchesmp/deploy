generator client {
  provider = "prisma-client-js"
}

generator next_crud {
  provider = "npx prisma-next-crud-generator"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model veiculo {
  placa     String  @id @db.VarChar
  motorista String? @db.VarChar
  latitude  String? @db.VarChar
  longitude String? @db.VarChar
}
